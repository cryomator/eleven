{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "member-access": false,
    "member-ordering": false,
    "arrow-parens": false,
    "no-namespace": false,
    "no-shadowed-variable": false,
    "interface-name": false,
    "max-classes-per-file": false,
    "object-literal-sort-keys": false,
    "no-console": false,
    "max-line-length": false,
    "ban-types": false,
    "no-angle-bracket-type-assertion": false,
    "array-type": false,
    "no-empty": false,
    "curly": [
      true,
      "ignore-same-line"
    ],
    "ordered-imports": true,
    "quotemark": {
      "severity": "default",
      "options": [
        "double"
      ]
    },
    "semicolon": false
  },
  "rulesDirectory": []
}